{
  "name": "tal-vision",
  "version": "0.1.1",
  "description": "A TAL application for Lancaster University's IPTV service, Vision.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -e js,css,hbs,css,less index.js",
    "start-prod": "node index.js",
    "postinstall": "npm install bower && bower install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "config": {
    "port": "3000"
  },
  "tal": {
    "default": {
      "application_id": "lancaster-vision"
    }
  },
  "keywords": [
    "tv",
    "bbc",
    "bbcrd",
    "second",
    "screen"
  ],
  "author": "Ross Wilson",
  "contributors": [
    "Thomas Parisot",
    "Anthony Onumonu"
  ],
  "license": "Apache-2",
  "dependencies": {
    "connect-redis": "~1.5.0",
    "cookie-parser": "^1.3.2",
    "express": "~3.13.0",
    "express-hbs": "~0.7.10",
    "cookie-session": "expressjs/cookie-session",
    "request": "~2.37.0",
    "tal": "~0.2.0",
    "tal-config": "~0.1.0"
  },
  "devDependencies": {
    "nodemon": "~1.2.1"
  }
}
